<!DOCTYPE html>
<html>
<head>
  <title>Idea Box</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |type, message| %>
    <%= content_tag :div, message %>
  <% end %>
  <%= yield %>
</body>
</html>
